{
    "name": "cakedc/cakephp-email-toolkit",
    "description": "Email Toolkit Plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "oauth2",
        "auth",
        "authentication",
        "cakedc"
    ],
    "homepage": "https://github.com/CakeDC/cakephp-email-toolkit",
    "license": "MIT",
    "authors": [
        {
            "name": "CakeDC",
            "homepage": "http://www.cakedc.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/CakeDC/cakephp-email-toolkit/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/CakeDC/cakephp-email-toolkit/issues",
        "source": "https://github.com/CakeDC/cakephp-email-toolkit"
    },
    "require": {
        "cakephp/cakephp": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\EmailToolkit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\EmailToolkit\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
